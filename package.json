    "esbuild-jest": "^0.5.0",
    "esbuild-plugin-postcss2": "^0.1.2",
    "esbuild-sass-plugin": "^2.2.6",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^8.0.1",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
