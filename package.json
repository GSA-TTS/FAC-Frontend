{
  "name": "11ty-uswds",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest __tests__",
    "start": "npx gulp copyAssets && npx gulp watch & npx @11ty/eleventy --serve",
    "build": "npx gulp copyAssets && npx gulp compile && npx @11ty/eleventy",
    "federalist": "npm run build",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "test:e2e:ci": "npx start-server-and-test start 8080 cy:run",
    "eslint:check": "npx eslint --ignore-path .gitignore .",
    "eslint:fix": "npx eslint --fix --ignore-path .gitignore .",
    "stylelint:check": "npx stylelint --ignore-pattern uswds src/scss",
    "stylelint:fix": "npx stylelint --fix --ignore-pattern uswds src/scss",
    "check:all": "npm run eslint:check && npm run stylelint:check",
    "fix:all": "npm run eslint:fix && npm run stylelint:fix",
    "pa11y": "npx pa11y-ci",
    "pa11y:ci": "npx start-server-and-test start 8080 pa11y",
    "postinstall": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@cypress-audit/lighthouse": "^1.3.0",
    "@cypress-audit/pa11y": "^1.3.0",
    "@uswds/compile": "^1.0.0-beta.2",
    "@uswds/uswds": "^3.0.1",
    "concurrently": "^7.1.0",
    "cypress": "^9.5.4",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.38",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^8.0.1",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.4.1",
    "pa11y": "^6.2.3",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.8.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0"
  },
  "dependencies": {
    "esbuild-sass-plugin": "^2.2.6",
    "pa11y-ci": "^3.0.1"
  }
