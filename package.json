  "main": "index.js",
  "scripts": {
    "test": "jest __tests__",
    "build": "run-s clean build:*",
    "clean": "rm -rf ./_src",
    "start": "run-p dev:*",
    "get-forms": "git checkout test-data/main -- ./src/forms",
    "dev:11ty": "eleventy --serve --watch --incremental",
    "dev:postcss": "postcss _site/assets/scss/main.css -o _site/assets/scss/main.css --use autoprefixer --watch",
    "build:11ty": "npx @11ty/eleventy",
    "build:postcss": "postcss _site/assets/scss/main.css -o _site/assets/scss/main.css --use autoprefixer",
    "federalist": "npm run build",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "test:e2e:ci": "npx start-server-and-test start 8080 cy:run",
    "check:eslint": "npx eslint --ignore-path .gitignore .",
    "fix:eslint": "npx eslint --fix --ignore-path .gitignore .",
    "check:stylelint": "npx stylelint --ignore-pattern uswds src/scss",
    "fix:stylelint": "npx stylelint --fix --ignore-pattern uswds src/scss",
    "check-all": "run-p check:*",
    "fix-all": "run-p fix:*",
    "pa11y": "npx pa11y-ci",
    "pa11y:ci": "npx start-server-and-test start 8080 pa11y",
    "postinstall": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
